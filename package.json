{
  "name": "@dlax/airtable-sdk",
  "version": "1.0.0",
  "description": "A modern, fully-typed TypeScript SDK for the Airtable API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "airtable",
    "api",
    "sdk",
    "typescript",
    "client",
    "database"
  ],
  "private": false,
  "author": "Laxmikanta Nayak",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/laxmikanta415/airtable-sdk"
  },
  "bugs": {
    "url": "https://github.com/laxmikanta415/airtable-sdk/issues"
  },
  "homepage": "https://github.com/laxmikanta415/airtable-sdk#readme",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
